{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build goldfish x86_64",
            "type": "shell",
            "command": [
                "export PATH=$TOOLCHAIN:$PATH &&",
                "make x86_64_yap_debug_defconfig &&",
                "make -j12"
            ],
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "options": {
                "env": {
                    "ARCH": "x86_64",
                    "CROSS_COMPILE": "x86_64-linux-androidkernel-",
                    "TOOLCHAIN": "${cwd}/../../prebuilts/gcc/linux-x86/x86/x86_64-linux-android-4.9/bin"
                }
            },
        }
    ]
}